{
	"id": "nodebb-plugin-always-webp",
	"url": "https://github.com/MiraiForum/nodebb-plugin-alwaysWebp",
	"library": "./library.js",
	"hooks": [
		{
			"hook": "static:app.load", "method": "init"
		},
		{
			"hook": "static:api.routes", "method": "addRoutes"
		},
		{
			"hook": "filter:admin.header.build", "method": "addAdminNavigation"
		},
	  {
		"hook": "filter:uploadImage","method": "uploadImgHook"
	  }
	],
	"staticDirs": {
		"static": "./static"
	},
	"less": [
		"static/less/quickstart.less"
	],
	"scripts": [
		"static/lib/main.js"
	],
	"acpScripts": [
		"static/lib/acp-main.js"
	],
	"modules": {
		"../client/quickstart.js": "./static/lib/quickstart.js",
		"../admin/plugins/quickstart.js": "./static/lib/admin.js"
	},
	"templates": "static/templates"
}